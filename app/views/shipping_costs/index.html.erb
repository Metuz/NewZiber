<h1>Listing shipping_costs</h1>

<table>
  <thead>
    <tr>
      <th>Serial,</th>
      <th>Company,</th>
      <th>Report</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipping_costs.each do |shipping_cost| %>
      <tr>
        <td><%= shipping_cost.serial, %></td>
        <td><%= shipping_cost.company, %></td>
        <td><%= shipping_cost.report_id %></td>
        <td><%= shipping_cost.date %></td>
        <td><%= link_to 'Show', shipping_cost %></td>
        <td><%= link_to 'Edit', edit_shipping_cost_path(shipping_cost) %></td>
        <td><%= link_to 'Destroy', shipping_cost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping cost', new_shipping_cost_path %>
