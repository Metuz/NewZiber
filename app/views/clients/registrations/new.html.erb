<div class="container">
  <h2>Registro</h2>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>
    <div class="form-group">
      <%= f.label :clientame, "Nombre" %>
      <%= f.text_field :clientname, autofocus: true, :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :email, "Correo" %>
      <%= f.email_field :email, autofocus: true, :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :phone, "Telefono" %>
      <%= f.text_field :phone, autofocus: true, :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :address, "Direccion" %>
      <%= f.text_field :address, autofocus: true, :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :password, "ContraseÃ±a" %>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "off", :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :password_confirmation, "ConfirmaciÃ³n" %>
      <%= f.password_field :password_confirmation, autocomplete: "off", :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.submit "Aceptar", :class => "btn btn-success" %>
    </div>
  <% end %>
  <%= render "clients/shared/links" %>
</div>
