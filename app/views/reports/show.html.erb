<p id="notice"><%= notice %></p>
<% if user_signed_in? %>
  <% if current_user.receptionist? %>
    <%= render 'reports/receptionist/show' %>
  <% elsif current_user.technician? %>
    <%= render 'reports/technician/show' %>
  <% elsif current_user.admin? %>
    <%= render 'reports/admin/show' %>
  <% else %>
    <%= render 'reports/manager/show' %>
  <% end %>
<% else %>
  <%= render 'reports/client/show' %>
<% end %>
