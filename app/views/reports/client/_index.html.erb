<div class="fullsize">
  <div class="page-header">
    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <h1 class="page-title">Mis Reportes</h1>
        </div>
        <div class="col-md-6">
          <ol class="breadcrumb">
            <li><%= link_to "Home", root_path, :class => "disabled has-submenu" %></li>
            <li>Mis Reportes</li>
            <li><%= link_to "Generar reporte", new_report_path %></li>
          </ol>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="main-wrapper">
  <div class="container container-inner"> <!-- Container -->
    <div class="row">
      <div class="col-md-12">
        <div class="table-responsive">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>Clave</th>
                <th>Sucursal</th>
                <th>Status</th>
              </tr>
            </thead>
            <tbody>
              <% @reports.each do |report| %>
                <tr>
                  <td><%= link_to report do %><i class="fa fa-eye"></i><% end %><%= report.pin %></td>
                  <td><%= report.location.name %></td>
                  <td><%= report.fninished %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
