<div class="fullsize">
  <div class="page-header">
    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <h3 class="page-title">Reportes Sucursal <%= current_user.location.name %></h3>
        </div>
        <div class="col-md-6">
          <ol class="breadcrumb">
            <li><%= link_to "Reportes", reports_path, :class => "disabled has-submenu" %></li>
          </ol>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="main-wrapper" id="admin">
  <div class="row">
    <ul class="nav nav-tabs" role="tablist">
      <li role="presentation" class="active"><a href="#asignados" aria-controls="asignados" role="tab" data-toggle="tab">Reportes Asignados</a></li>
      <li role="presentation"><a href="#terminados" aria-controls="terminados" role="tab" data-toggle="tab">Reportes Finalizados</a></li>
    </ul>
    <div class="tab-content">
      <div role="tabpanel" class="tab-pane fade in active" id="asignados">
        <%= render 'reports/technician/asignados' %>
      </div>
      <div role="tabpanel" class="tab-pane fade" id="terminados">
        <%= render 'reports/technician/terminados' %>
      </div>
    </div>
  </div>
</div>
