<div class="table-responsive">
  <table class="datatable">
    <thead>
      <tr>
        <th>Clave</th>
        <th>Clente</th>
        <th>Tecnico</th>
        <th>Modelo</th>
        <th>No. de Serie</th>
      </tr>
    </thead>
    <tbody>
      <% @reports.delivered_in.not_finished.each do |report| %>
        <tr>
          <td><%= link_to report do %><i class="fa fa-eye"></i><% end %><%= report.pin %></td>
          <td><%= report.client.name %></td>
          <td><%= report.user.name %></td>
          <td><%= report.model %></td>
          <td><%= report.serial %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
