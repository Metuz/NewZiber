
  <% if current_user.is_a?(Secretary) %>
    <%= render 'reports/receptionist/index' %>
  <% elsif current_user.is_a?(Technician) %>
    <%= render 'reports/technician/index' %>
  <% elsif current_user.is_a?(Admin) %>
    <%= render 'reports/admin/index' %>
  <% elsif current_user.is_a?(Manager) %>
    <%= render 'reports/manager/index' %>
  <% else %>
    <%= render 'reports/client/index' %>
  <% end %>
